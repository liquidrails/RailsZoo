<table width = "<%= TABLE_WIDTH %>" align="center" border="0" cellspacing="0" cellpadding="5" bgcolor="#ffffff">


<table width = 100%>

<tr><td colspan=5><table bgcolor=#cf6 cellpadding=0 width=100%>
  
  <tr span class="listings"> 
    <th  width=20%><span class="directory_header">Type</th>
    <th  width=20%><span class="directory_header">Operator</th>
    <th  width=20%><span class="directory_header">Details</th>
    <th  width=20%><span class="directory_header">Phone</th>
    <th width = 20%>&nbsp;</th>
 
  </tr>

</table></td></tr>
<tr height=7px><td></td></tr>

<% cint = -1 %>
<% for posting in @postings %>
<% cint = cint*-1 %>
	<% if cint == -1 %>
  <tr>
	<% else %>
  <tr bgcolor = #ffb>
	<% end %>

    <td><i><%= posting.subcategory.subname.name %></i>&nbsp;</td>
    <td><b><%= link_to posting.organizer, posting %></b></td>
    <td><%= posting.details %></td>
    <td><%= link_to 'Edit', edit_posting_path(posting) %></td>
    <td><%= link_to 'Delete', user_delete_path(@location, posting), :confirm => 'Are you sure?' %></td>
  </tr>

<% end %>
</table>

<br><br>

<span class="paginate">
  <%= will_paginate @postings, :params=>{:month=>@month, :year=>@year}%>

<br />


