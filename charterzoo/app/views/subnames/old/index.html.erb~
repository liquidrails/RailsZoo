<h1>Listing subnames</h1>

<table cellspacing=3>
  <tr>
    <td><h3>ID</h3></td>
    <td><h3>Name</h3></td>
  </tr>

<% for subname in @subnames %>
  <tr>
    <td><%= subname.id %></td>
    <td><%= subname.name %></td>
    <td><%= link_to 'Show', subname %></td>
    <td><%= link_to 'Edit', edit_subname_path(subname) %></td>
    <td><%= link_to 'Destroy', subname, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New subname', new_subname_path %>
