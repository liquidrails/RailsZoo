_erbout.concat "  <th colspan=1>"; _erbout.concat(( location.name ).to_s); _erbout.concat " </th>\n"
_erbout.concat " ";  end ; _erbout.concat "\n"
_erbout.concat "</tr>\n"
 subcats = category.subcategories.select{|v| v.location_id == 0 }  ; _erbout.concat "\n"
 subcat_array = subcats.sort {|a,b| a.subname.name <=> b.subname.name} ; _erbout.concat "\n"
 for subcat in subcat_array ; _erbout.concat "\n"
_erbout.concat "<tr><th>"; _erbout.concat(( subcat.subname.name ).to_s); _erbout.concat "</th>\n"
_erbout.concat "   ";  for location in @locations ; _erbout.concat "\n"
_erbout.concat "    ";  if location.id == subcat.location_id ; _erbout.concat "\n"
_erbout.concat "       <td bgcolor = #ccffcc>"; _erbout.concat(( link_to "On", {:controller=>"subcategories", :action =>"destroy", :id => subcat.id} ).to_s); _erbout.concat "\n"
_erbout.concat "\t\n"
_erbout.concat "</td>\n"
_erbout.concat "    ";  else ; _erbout.concat "\n"
_erbout.concat "       <td bgcolor = #ffccff>\n"
_erbout.concat "        ";  @subcategory = Subcategory.new ; _erbout.concat "\n"
_erbout.concat "\t";  form_for(@subcategory):url => { :action => "add" }  do |f| ; _erbout.concat "\n"
_erbout.concat "\t\t"; _erbout.concat(( f.hidden_field :category_id, :value => category.id ).to_s); _erbout.concat "\n"
_erbout.concat "\t\t"; _erbout.concat(( f.hidden_field :location_id, :value => location.id ).to_s); _erbout.concat "\n"
_erbout.concat "\t\t"; _erbout.concat(( f.hidden_field :subname_id, :value => subcat.subname.id ).to_s); _erbout.concat "\n"
_erbout.concat " \t"; _erbout.concat(( f.submit "Off" ).to_s); _erbout.concat "\n"
_erbout.concat "       </td>\n"
_erbout.concat "    ";  end ; _erbout.concat "\n"
_erbout.concat "  ";  end ; _erbout.concat "\n"
_erbout.concat "\n"
 end ; _erbout.concat "\n"
_erbout.concat "</tr>\n"
 end ; _erbout.concat "\n"
 end ; _erbout.concat "\n"
_erbout.concat "</table>\n"
_erbout.concat "\n"
_erbout
